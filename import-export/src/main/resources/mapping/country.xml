<beanio xmlns="http://www.beanio.org/2012/03"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <stream name="countries" format="csv">
    <typeHandler name="phoneHandler" class="uk.co.bluegecko.beanio.ListOfIntTypeHandler"/>
    <typeHandler name="currencyHandler" class="uk.co.bluegecko.beanio.SetOfStringTypeHandler"/>
    <typeHandler name="languageHandler" class="uk.co.bluegecko.beanio.SetOfStringTypeHandler"/>
    <record name="country" class="uk.co.bluegecko.data.model.CountryData">
      <field name="code"/>
      <field name="name"/>
      <field name="nativeName"/>
      <field name="phones" typeHandler="phoneHandler"/>
      <field name="continent"/>
      <field name="capital"/>
      <field name="currencies" typeHandler="currencyHandler"/>
      <field name="languages" typeHandler="languageHandler"/>
    </record>
  </stream>
</beanio>